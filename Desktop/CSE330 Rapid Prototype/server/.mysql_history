CREATE TABLE `courses` (
  `school_code` ENUM('L','B','A','F','E','T','I','W','S','U','M') NOT NULL,
  `dept_id` INT(10) UNSIGNED NOT NULL,
  `course_code` CHAR(5) NOT NULL,
  `name` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`school_code`, `dept_id`, `course_code`),
  CONSTRAINT `courses_ibfk_1` FOREIGN KEY (`school_code`, `dept_id`) REFERENCES `departments` (`school_code`, `dept_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;
CREATE TABLE `courses` (   `school_code` ENUM('L','B','A','F','E','T','I','W','S','U','M') NOT NULL,   `dept_id` INT(10) UNSIGNED NOT NULL,   `course_code` CHAR(5) NOT NULL,   `name` VARCHAR(100) NOT NULL,   PRIMARY KEY (`school_code`, `dept_id`, `course_code`),   CONSTRAINT `courses_ibfk_1` FOREIGN KEY (`school_code`, `dept_id`) REFERENCES `departments` (`school_code`, `dept_id`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;
